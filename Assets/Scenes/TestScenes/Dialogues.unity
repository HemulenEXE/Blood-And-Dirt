  Nodes:
  - npcName: "\u0418\u043D\u0441\u0442\u0440\u0443\u043A\u0442\u043E\u0440"
    npcText: "<b>\u0418\u0442\u0430\u043A, \u0441\u043E\u043B\u0434\u0430\u0442\u044B,</b>
      <color=green>\u0432\u044B \u0443\u0441\u043F\u0435\u0448\u043D\u043E \u0432\u044B\u043F\u043E\u043B\u043D\u0438\u043B\u0438
      \u043C\u0438\u0441\u0441\u0438\u044E</color> \u0438 \u0437\u0430 \u044D\u0442\u043E
      \u0434\u043E\u0441\u0442\u043E\u0439\u043D\u044B \u043D\u0430\u0433\u0440\u0430\u0434\u044B.
      \u0412\u0441\u0435 \u0432\u044B \u0437\u043D\u0430\u0435\u0442\u0435, \u0447\u0442\u043E
      \u043C\u044B \u0442\u0435\u0441\u043D\u043E \u0441\u043E\u0442\u0440\u0443\u0434\u043D\u0438\u0447\u0430\u0435\u043C
      \u0441 \u0445\u0440\u0430\u043C\u043E\u043C \u0438 \u043D\u0430 \u0434\u043D\u044F\u0445
      \u043D\u0430\u0448\u0435 \u043A\u043E\u043C\u0430\u043D\u0434\u043E\u0432\u0430\u043D\u0438\u0435
      \u0441\u043E\u0432\u043C\u0435\u0441\u0442\u043D\u043E \u0441 \u0437\u0430\u0432\u0435\u0434\u0443\u044E\u0449\u0438\u043C\u0438
      \u043C\u0435\u0441\u0442\u043D\u043E\u0439 \u0441\u0432\u044F\u0442\u044B\u043D\u0438
      \u0440\u0435\u0448\u0438\u043B\u043E \u043E\u0440\u0433\u0430\u043D\u0438\u0437\u043E\u0432\u0430\u0442\u044C
      \u0432\u0441\u0435\u043E\u0431\u0449\u0435\u0435 \u0440\u0435\u043B\u0438\u0433\u0438\u043E\u0437\u043D\u043E\u0435
      \u043F\u0440\u0430\u0437\u0434\u043D\u0435\u0441\u0442\u0432\u043E \u0434\u043B\u044F
      \u043F\u043E\u0434\u043D\u044F\u0442\u0438\u044F \u043F\u0430\u0442\u0440\u0438\u043E\u0442\u0438\u0447\u0435\u0441\u043A\u043E\u0433\u043E
      \u0434\u0443\u0445\u0430 \u0438 \u0443\u043A\u0440\u0435\u043F\u043B\u0435\u043D\u0438\u044F
      \u0432\u0435\u0440\u044B. \u041F\u043E \u0442\u0430\u043A\u043E\u043C\u0443
      \u043F\u043E\u0432\u043E\u0434\u0443, \u043D\u0430 \u0432\u0440\u0435\u043C\u044F
      \u043C\u0435\u0440\u043E\u043F\u0440\u0438\u044F\u0442\u0438\u044F, \u043D\u0430\u0448\u0435\u0439
      \u0440\u043E\u0442\u0435 \u0440\u0430\u0437\u0440\u0435\u0448\u0430\u0435\u0442\u0441\u044F
      \u043F\u043E\u043A\u0438\u043D\u0443\u0442\u044C \u0431\u0430\u0437\u0443 \u0438
      \u0445\u043E\u0440\u043E\u0448\u0435\u043D\u044C\u043A\u043E \u043F\u043E\u0432\u0435\u0441\u0435\u043B\u0438\u0442\u044C\u0441\u044F!
      <b>\u041F\u0440\u0430\u0437\u0434\u043D\u0438\u043A \u0431\u0443\u0434\u0435\u0442
      \u043F\u0440\u043E\u0432\u0435\u0434\u0435\u043D \u0441\u0435\u0433\u043E\u0434\u043D\u044F
      \u0440\u044F\u0434\u043E\u043C \u0441 \u0445\u0440\u0430\u043C\u043E\u043C
      \u043D\u0430 \u0433\u043B\u0430\u0432\u043D\u043E\u0439 \u043F\u043B\u043E\u0449\u0430\u0434\u0438.</b>
      \u041F\u043E\u0441\u043B\u0435 \u043F\u0440\u0430\u0437\u0434\u043D\u0438\u043A\u0430
      \u0432\u0430\u0441 \u043E\u0442\u043F\u0440\u0430\u0432\u044F\u0442 \u043E\u0431\u0440\u0430\u0442\u043D\u043E
      \u043A \u0441\u0435\u0431\u0435 \u043D\u0430 \u0431\u0430\u0437\u0443. \u041D\u0430
      \u044D\u0442\u043E\u0439 \u0440\u0430\u0434\u043E\u0441\u0442\u043D\u043E\u0439
      \u043D\u043E\u0442\u0435 \u043C\u043E\u0436\u0435\u0442\u0435 \u0431\u044B\u0442\u044C
      \u0441\u0432\u043E\u0431\u043E\u0434\u043D\u044B!"
    exit: 1
