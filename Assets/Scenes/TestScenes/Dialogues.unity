  - npcName: "\u0414\u043E\u043A\u0442\u043E\u0440 \u041F\u043B\u0430\u043D \u041C\u044E\u043B\u043B\u0435\u0440"
    npcText: "\u0422\u044B, \u0411\u0443\u0434\u0434\u0430, \u0432\u0438\u0434\u0438\u043C\u043E
      \u0435\u0449\u0451 \u043D\u0435 \u043F\u043E\u043D\u044F\u043B, \u043A\u0430\u043A
      \u043E\u0431\u0441\u0442\u043E\u044F\u0442 \u0434\u0435\u043B\u0430. \u0412\u043F\u0440\u043E\u0447\u0435\u043C,
      \u043D\u0435 \u0432\u043E\u043B\u043D\u0443\u0439\u0441\u044F, \u044F \u0440\u0430\u0437\u044A\u044F\u0441\u043D\u044E.
      \u041C\u043D\u0435 \u0438\u0437\u0432\u0435\u0441\u0442\u043D\u044B \u0432\u0441\u0435
      \u0432\u0430\u0448\u0438 \u0433\u0440\u044F\u0437\u043D\u044B\u0435 \u0442\u0430\u0439\u043D\u044B,
      \u044F \u043F\u0440\u043E\u0432\u043E\u0434\u0438\u043B \u0438\u0441\u0441\u043B\u0435\u0434\u043E\u0432\u0430\u043D\u0438\u044F
      \u0438 \u0437\u043D\u0430\u044E, \u043A\u0430\u043A\u043E\u0439 \u0434\u0440\u044F\u043D\u044C\u044E
      \u0432\u044B \u043F\u0438\u0447\u043A\u0430\u0435\u0442\u0435 \u0441\u0432\u043E\u0438\u0445
      \u0441\u043E\u043B\u0434\u0430\u0442. \u0411\u0435\u0437\u0443\u0441\u043B\u043E\u0432\u043D\u043E,
      \u043E\u0442\u043B\u0438\u0447\u043D\u043E\u0435 \u0441\u0440\u0435\u0434\u0441\u0442\u0432\u043E
      \u0434\u043B\u044F \u0431\u043E\u044F \u2013 \u0442\u0435\u0441\u0442\u043E\u0441\u0442\u0435\u0440\u043E\u043D
      \u0438 \u0430\u0434\u0440\u0435\u043D\u0430\u043B\u0438\u043D \u0437\u0430\u0448\u043A\u0430\u043B\u0438\u0432\u0430\u044E\u0442,
