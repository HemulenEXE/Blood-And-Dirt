  Nodes:
  - npcName: "\u0417\u0430\u043A\u043B\u044E\u0447\u0451\u043D\u043D\u044B\u0439"
    npcText: 
    exit: 0
    playerAnswer:
    - text: "\u041A\u0430\u043A \u0432\u044B \u0441\u044E\u0434\u0430 \u043F\u043E\u043F\u0430\u043B\u0438?"
      toNode: 1
      toScene: 0
      exit: 0
    - text: "\u0412\u0430\u043C \u043D\u043E\u0440\u043C\u0430\u043B\u044C\u043D\u043E?
        \u0412\u044B\u0433\u043B\u044F\u0434\u0438\u0442\u0435 \u0431\u043B\u0435\u0434\u043D\u044B\u043C."
      toNode: 2
      toScene: 0
      exit: 0
    - text: "\u0414\u0430\u0432\u043D\u043E \u0432\u044B \u0437\u0434\u0435\u0441\u044C
        \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u0435\u0441\u044C? "
      toNode: 4
      toScene: 0
      exit: 0
  - npcName: "\u0417\u0430\u043A\u043B\u044E\u0447\u0451\u043D\u043D\u044B\u0439"
    npcText: "<b>\u041D\u0435 \u0442\u0432\u043E\u0451 \u0441\u043E\u0431\u0430\u0447\u044C\u0435
      \u0434\u0435\u043B\u043E, \u043E\u0442\u0432\u0430\u043B\u0438.</b>"
    exit: 1
    playerAnswer: []
  - npcName: "\u0417\u0430\u043A\u043B\u044E\u0447\u0451\u043D\u043D\u044B\u0439"
    npcText: "<wave>\u042F \u0433\u043E\u043B\u043E\u0434\u0435\u043D.</wave> \u0423
      \u0442\u0435\u0431\u044F \u0435\u0441\u0442\u044C \u0447\u0442\u043E-\u043D\u0438\u0431\u0443\u0434\u044C
      \u0434\u043B\u044F \u043C\u0435\u043D\u044F? "
    exit: 0
    playerAnswer:
    - text: "\u0423 \u043C\u0435\u043D\u044F \u043D\u0438\u0447\u0435\u0433\u043E
        \u043D\u0435\u0442\u2026 "
      toNode: 3
      toScene: 0
      exit: 0
  - npcName: "\u041E\u0445\u0440\u0430\u043D\u043D\u0438\u043A"
    npcText: "<b>\u041F\u0440\u0435\u043A\u0440\u0430\u0442\u0438\u0442\u044C \u0440\u0430\u0437\u0433\u043E\u0432\u043E\u0440\u044B!</b>"
    exit: 1
    playerAnswer: []
  - npcName: "\u0417\u0430\u043A\u043B\u044E\u0447\u0451\u043D\u043D\u044B\u0439"
    npcText: "\u041D\u0435 \u0437\u043D\u0430\u044E \u0442\u043E\u0447\u043D\u043E,
      \u043C\u043E\u0436\u0435\u0442 \u043C\u0435\u0441\u044F\u0446, \u043C\u043E\u0436\u0435\u0442
      \u0434\u0432\u0430. "
    exit: 0
    playerAnswer:
    - text: "\u041D\u0435 \u043F\u0440\u043E\u0431\u043E\u0432\u0430\u043B\u0438
        \u0441\u0431\u0435\u0436\u0430\u0442\u044C?"
      toNode: 5
      toScene: 0
      exit: 0
  - npcName: "\u041E\u0445\u0440\u0430\u043D\u043D\u0438\u043A"
