      \u041D\u0435 \u0438\u0437\u0434\u0430\u0432\u0430\u0439\u0442\u0435 \u0433\u0440\u043E\u043C\u043A\u0438\u0445
      \u0437\u0432\u0443\u043A\u043E\u0432 \u0438 \u043D\u0435 \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u0443\u0439\u0442\u0435
      \u043E\u0433\u043D\u0435\u0441\u0442\u0440\u0435\u043B. <b>\u0412\u043D\u0435\u0437\u0430\u043F\u043D\u043E\u0441\u0442\u044C
      \u2013 \u043D\u0430\u0448\u0435 \u0433\u043B\u0430\u0432\u043D\u043E\u0435
      \u043E\u0440\u0443\u0436\u0438\u0435, \u043D\u0435 \u0434\u0430\u0439\u0442\u0435
      \u0432\u0440\u0430\u0433\u0443 \u043F\u043E\u0434\u0433\u043E\u0442\u043E\u0432\u0438\u0442\u044C\u0441\u044F.</b>
      \u041A\u043E\u0433\u0434\u0430 \u043F\u0440\u043E\u0431\u0435\u0440\u0451\u0442\u0435\u0441\u044C
      \u0432\u043D\u0443\u0442\u0440\u044C, \u0437\u0430\u0447\u0438\u0441\u0442\u0438\u0442\u0435
      \u0432\u0441\u0435 \u043F\u043E\u043C\u0435\u0449\u0435\u043D\u0438\u044F.
      \u041D\u0430 \u044D\u0442\u043E\u043C \u0432\u0441\u0451, \u043F\u043E \u043C\u0430\u0448\u0438\u043D\u0430\u043C! "
