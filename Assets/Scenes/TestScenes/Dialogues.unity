  - npcName: "\u0421\u0432\u044F\u0442\u043E\u0439 \u0411\u0443\u0434\u0434\u0430"
    npcText: "\u0417\u0430\u0432\u0438\u0441\u0438\u0442 \u043E\u0442 \u0442\u043E\u0433\u043E
      \u043D\u0430\u0441\u043A\u043E\u043B\u044C\u043A\u043E \u0432\u044B \u0431\u0443\u0434\u0435\u0442\u0435
      \u0441\u0433\u043E\u0432\u043E\u0440\u0447\u0438\u0432\u044B \u0438 \u043F\u043E\u043B\u0435\u0437\u043D\u044B.
      \u0422\u043E\u043B\u043F\u0430 \u0432\u0441\u0435-\u0442\u0430\u043A\u0438
      \u0442\u0440\u0435\u0431\u0443\u0435\u0442 <color=red>\u043A\u0440\u043E\u0432\u0438.</color>
      \u0411\u0443\u0434\u0435\u0442 \u043E\u0431\u0438\u0434\u043D\u043E \u043E\u0441\u0442\u0430\u0432\u043B\u044F\u0442\u044C
      \u0438\u0445 \u0431\u0435\u0437 \u0437\u0440\u0435\u043B\u0438\u0449\u0430.
      \u041D\u043E \u043F\u043E\u0432\u0442\u043E\u0440\u044E\u0441\u044C, \u0432\u0441\u0451
      \u0432 \u0432\u0430\u0448\u0438\u0445 \u0440\u0443\u043A\u0430\u0445, \u0434\u043E\u043A\u0442\u043E\u0440. "
    exit: 0
