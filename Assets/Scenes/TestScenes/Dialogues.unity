      exit: 0
  - npcName: "\u041F\u0440\u043E\u0432\u043E\u0436\u0430\u0442\u044B\u0439"
    npcText: "\u041E\u0442\u043B\u0438\u0447\u043D\u043E! \u0417\u043D\u0430\u0447\u0438\u0442
      \u0441 \u044D\u0442\u043E\u0433\u043E \u0434\u043D\u044F, <b>\u041B\u0443\u043A\u0430</b>,
      \u0442\u044B \u0440\u044F\u0434\u043E\u0432\u043E\u0439 \u0430\u0440\u043C\u0438\u0438
      <color=red>\u0421\u0432\u044F\u0442\u043E\u0433\u043E \u0411\u0443\u0434\u0434\u044B.</color>
      \u041D\u043E\u0441\u0438 \u044D\u0442\u043E \u0437\u0432\u0430\u043D\u0438\u0435
      \u0441 \u0447\u0435\u0441\u0442\u044C\u044E \u0438 \u0433\u043E\u0440\u0434\u043E\u0441\u0442\u044C\u044E.
      \u0417\u0430\u0432\u0442\u0440\u0430 \u044F \u043F\u0440\u043E\u0432\u0435\u0434\u0443
      \u0442\u0435\u0431\u0435 \u0438\u043D\u0441\u0442\u0440\u0443\u043A\u0442\u0430\u0436,
      \u0430 \u0441\u0435\u0433\u043E\u0434\u043D\u044F \u043C\u043E\u0436\u0435\u0448\u044C
      \u043E\u0442\u0434\u043E\u0445\u043D\u0443\u0442\u044C \u0438 \u043F\u043E\u0437\u043D\u0430\u043A\u043E\u043C\u0438\u0442\u044C\u0441\u044F
      \u0441 \u043E\u0441\u0442\u0430\u043B\u044C\u043D\u044B\u043C\u0438 \u0440\u0435\u0431\u044F\u0442\u0430\u043C\u0438. "
    exit: 0
    playerAnswer:
    - text: 
      toNode: 5
      toScene: 0
      exit: 0
  - npcName: "\u041F\u0440\u043E\u0432\u043E\u0436\u0430\u0442\u044B\u0439"
    npcText: "\u041A\u0430\u043A\u043E\u0439 \u0442\u044B \u043F\u0440\u0438\u0432\u0435\u0440\u0435\u0434\u043B\u0438\u0432\u044B\u0439.
      \u041B\u0430\u0434\u043D\u043E, \u043D\u0435 \u0431\u0443\u0434\u0443 \u043D\u0430\u0441\u0442\u0430\u0438\u0432\u0430\u0442\u044C.
      \u041A\u0430\u043A \u043D\u0430\u0441\u0447\u0451\u0442 \u0438\u043C\u0435\u043D\u0438
      <b>\u0414\u0436\u0435\u0440\u043E\u043C?</b> \u041C\u043E\u0435\u0433\u043E
      \u0431\u0440\u0430\u0442\u0430 \u0442\u0430\u043A \u0437\u043E\u0432\u0443\u0442,
      \u0442\u043E\u0436\u0435 \u043E\u0447\u0435\u043D\u044C \u0445\u043E\u0440\u043E\u0448\u0435\u0435
      \u0438\u043C\u044F. "
    exit: 0
    playerAnswer:
    - text: "<color=green>\u0421\u043E\u0433\u043B\u0430\u0441\u0438\u0442\u044C\u0441\u044F.</color>"
      toNode: 5
      toScene: 0
      exit: 0
  - npcName: "\u041F\u0440\u043E\u0432\u043E\u0436\u0430\u0442\u044B\u0439"
    npcText: 
    exit: 0
    playerAnswer:
    - text: "\u0427\u0442\u043E \u0431\u0443\u0434\u0435\u0442 \u043F\u043E\u0441\u043B\u0435
        \u0438\u043D\u0441\u0442\u0440\u0443\u043A\u0442\u0430\u0436\u0430?"
      toNode: 6
      toScene: 0
      exit: 0
    - text: "\u0413\u0434\u0435 \u044F \u0431\u0443\u0434\u0443 \u0441\u043B\u0443\u0436\u0438\u0442\u044C?"
      toNode: 7
      toScene: 0
      exit: 0
  - npcName: "\u041F\u0440\u043E\u0432\u043E\u0436\u0430\u0442\u044B\u0439"
    npcText: "\u041A\u0430\u043A \u043D\u0430\u0443\u0447\u0438\u0448\u044C\u0441\u044F
      \u043E\u0441\u043D\u043E\u0432\u0430\u043C, \u0441\u0440\u0430\u0437\u0443
      \u0432 \u0431\u043E\u0439 \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u0448\u044C\u0441\u044F.
      \u0421\u0438\u0442\u0443\u0430\u0446\u0438\u044F \u0441\u0435\u0439\u0447\u0430\u0441
      \u043D\u0435\u0441\u0442\u0430\u0431\u0438\u043B\u044C\u043D\u0430\u044F, \u0431\u043E\u0439\u0446\u044B
      \u043D\u0443\u0436\u043D\u044B \u043F\u043E\u0437\u0430\u0440\u0435\u0437,
      \u0442\u0430\u043A \u0447\u0442\u043E \u043D\u043E\u0432\u0438\u0447\u043A\u043E\u0432
      \u0434\u043E\u043B\u0433\u043E \u0432 \u0437\u0430\u043F\u0430\u0441\u0435
      \u043D\u0435 \u0434\u0435\u0440\u0436\u0430\u0442. \u0427\u0435\u0440\u0435\u0437
      \u0434\u0435\u043D\u044C-\u0434\u0432\u0430 \u0431\u0443\u0434\u0435\u0448\u044C
      \u0443\u0436\u0435 \u0435\u0440\u0435\u0442\u0438\u043A\u043E\u0432 \u0441\u0442\u0440\u0435\u043B\u044F\u0442\u044C.
      <b>\u0421\u043E\u0441\u043A\u0443\u0447\u0438\u0442\u044C\u0441\u044F \u043D\u0435
      \u0443\u0441\u043F\u0435\u0435\u0448\u044C!</b> \u042F \u0442\u0443\u0442 \u0441\u043B\u044B\u0448\u0430\u043B,
      \u0447\u0442\u043E \u0432 \u0440\u0430\u0439\u043E\u043D\u0435 \u0425\u0430\u0440\u0434\u0435\u043D\u0430
      \u043D\u0430\u043F\u0430\u0434\u0435\u043D\u0438\u0435 \u043F\u043B\u0430\u043D\u0438\u0440\u0443\u0435\u0442\u0441\u044F,
      \u0442\u0435\u0431\u044F \u0442\u0443\u0434\u0430 \u0441\u043A\u043E\u0440\u0435\u0435
      \u0432\u0441\u0435\u0433\u043E \u043F\u0435\u0440\u0435\u043A\u0438\u043D\u0443\u0442.
      \u041D\u043E \u044D\u0442\u043E \u0442\u043E\u043B\u044C\u043A\u043E \u043C\u0435\u0436\u0434\u0443
      \u043D\u0430\u043C\u0438, \u044F \u0442\u0435\u0431\u0435 \u043D\u0438\u0447\u0435\u0433\u043E
      \u043D\u0435 \u0433\u043E\u0432\u043E\u0440\u0438\u043B! \u041F\u043E\u043D\u044F\u043B? "
    exit: 1
    playerAnswer: []
  - npcName: "\u041F\u0440\u043E\u0432\u043E\u0436\u0430\u0442\u044B\u0439"
    npcText: "\u041A\u043E\u043C\u0430\u043D\u0434\u0438\u0440 \u043E\u0442\u0431\u0438\u0440\u0430\u043B
      \u0431\u043E\u0439\u0446\u043E\u0432, \u0430 \u043D\u0435 \u0442\u0435\u0445\u043D\u0438\u0447\u0435\u0441\u043A\u0438\u0439
      \u043F\u0435\u0440\u0441\u043E\u043D\u0430\u043B, \u0442\u0430\u043A \u0447\u0442\u043E-\u0442\u043E
      \u0434\u043E\u043B\u0433\u043E \u043E\u0442\u0441\u0438\u0436\u0438\u0432\u0430\u0442\u044C\u0441\u044F
      \u043D\u0430 \u0431\u0430\u0437\u0435 \u043D\u0435 \u043F\u043E\u043B\u0443\u0447\u0438\u0442\u044C\u0441\u044F.
      \u0427\u0435\u0440\u0435\u0437 \u0434\u0435\u043D\u044C-\u0434\u0432\u0430...
      \u041A\u0430\u043A \u043D\u0430\u0443\u0447\u0438\u0448\u044C\u0441\u044F \u043E\u0441\u043D\u043E\u0432\u0430\u043C,
      \u0441\u0440\u0430\u0437\u0443 \u0432 \u0431\u043E\u0439 \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u0448\u044C\u0441\u044F.
      \u0421\u0438\u0442\u0443\u0430\u0446\u0438\u044F \u0441\u0435\u0439\u0447\u0430\u0441
      \u043D\u0435\u0441\u0442\u0430\u0431\u0438\u043B\u044C\u043D\u0430\u044F, \u0431\u043E\u0439\u0446\u044B
      \u043D\u0443\u0436\u043D\u044B \u043F\u043E\u0437\u0430\u0440\u0435\u0437,
      \u0442\u0430\u043A \u0447\u0442\u043E \u043D\u043E\u0432\u0438\u0447\u043A\u043E\u0432
      \u0434\u043E\u043B\u0433\u043E \u0432 \u0437\u0430\u043F\u0430\u0441\u0435
      \u043D\u0435 \u0434\u0435\u0440\u0436\u0430\u0442. \u0427\u0435\u0440\u0435\u0437
      \u0434\u0435\u043D\u044C-\u0434\u0432\u0430 \u0431\u0443\u0434\u0435\u0448\u044C
      \u0443\u0436\u0435 \u0435\u0440\u0435\u0442\u0438\u043A\u043E\u0432 \u0441\u0442\u0440\u0435\u043B\u044F\u0442\u044C.
      <b>\u0421\u043E\u0441\u043A\u0443\u0447\u0438\u0442\u044C\u0441\u044F \u043D\u0435
      \u0443\u0441\u043F\u0435\u0435\u0448\u044C!</b> \u042F \u0442\u0443\u0442 \u0441\u043B\u044B\u0448\u0430\u043B,
      \u0447\u0442\u043E \u0432 \u0440\u0430\u0439\u043E\u043D\u0435 \u0425\u0430\u0440\u0434\u0435\u043D\u0430
      \u043D\u0430\u043F\u0430\u0434\u0435\u043D\u0438\u0435 \u043F\u043B\u0430\u043D\u0438\u0440\u0443\u0435\u0442\u0441\u044F,
      \u0442\u0435\u0431\u044F \u0442\u0443\u0434\u0430 \u0441\u043A\u043E\u0440\u0435\u0435
      \u0432\u0441\u0435\u0433\u043E \u043F\u0435\u0440\u0435\u043A\u0438\u043D\u0443\u0442.
      \u041D\u043E \u044D\u0442\u043E \u0442\u043E\u043B\u044C\u043A\u043E \u043C\u0435\u0436\u0434\u0443
      \u043D\u0430\u043C\u0438, \u044F \u0442\u0435\u0431\u0435 \u043D\u0438\u0447\u0435\u0433\u043E
      \u043D\u0435 \u0433\u043E\u0432\u043E\u0440\u0438\u043B! \u041F\u043E\u043D\u044F\u043B?  "
    exit: 1
    playerAnswer: []
