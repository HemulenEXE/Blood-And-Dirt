  - npcName: "\u0421\u0432\u044F\u0442\u043E\u0439 \u0411\u0443\u0434\u0434\u0430"
    npcText: "\u041D\u0443 \u0447\u0442\u043E \u0432\u044B, \u0434\u043E\u043A\u0442\u043E\u0440.
      \u041D\u0435 \u044F\u0437\u0432\u0438\u0442\u0435, \u044F \u0433\u043E\u0432\u043E\u0440\u044E
      \u0432\u043F\u043E\u043B\u043D\u0435 \u0441\u0435\u0440\u044C\u0451\u0437\u043D\u043E.
      \u041C\u044B \u043C\u043E\u0436\u0435\u043C \u043E\u0441\u0442\u0430\u0432\u0438\u0442\u044C
      \u0432\u0430\u0448\u0438 \u0433\u0440\u0435\u0445\u0438 \u043D\u0430 \u043E\u0442\u043A\u0443\u043F
      \u0413\u043E\u0441\u043F\u043E\u0434\u0443, \u0435\u0441\u043B\u0438 \u0432\u044B
      \u0441\u043E\u0438\u0437\u0432\u043E\u043B\u0438\u0442\u0435 \u043F\u0440\u0435\u0434\u043E\u0441\u0442\u0430\u0432\u0438\u0442\u044C
      \u043D\u0430\u043C \u043D\u0435\u043A\u043E\u0442\u043E\u0440\u0443\u044E \u0438\u043D\u0444\u043E\u0440\u043C\u0430\u0446\u0438\u044E. "
    exit: 0
