  - npcName: "\u0421\u0432\u044F\u0442\u043E\u0439 \u0411\u0443\u0434\u0434\u0430"
    npcText: "\u041A \u0442\u043E\u043C\u0443, \u0447\u0442\u043E \u0432\u0430\u0448\u0430
      \u0436\u0438\u0437\u043D\u044C \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u044C\u0441\u044F
      \u0432 \u0432\u0430\u0448\u0438\u0445 \u0440\u0443\u043A\u0430\u0445. \u041C\u044B
      \u0441\u043C\u043E\u0436\u0435\u043C \u0441\u043C\u044F\u0433\u0447\u0438\u0442\u044C
      \u043D\u0430\u043A\u0430\u0437\u0430\u043D\u0438\u0435, \u0435\u0441\u043B\u0438
      \u0432\u044B \u0441\u043E\u0433\u043B\u0430\u0441\u0438\u0442\u0435\u0441\u044C
      \u043F\u043E\u0439\u0442\u0438 \u043D\u0430\u043C \u043D\u0430 \u0432\u0441\u0442\u0440\u0435\u0447\u0443,
      \u0434\u043E\u043A\u0442\u043E\u0440. \u0427\u0442\u043E \u0434\u0443\u043C\u0430\u0435\u0442\u0435?
      \u0414\u043E\u0432\u043E\u043B\u044C\u043D\u043E \u0449\u0435\u0434\u0440\u043E\u0435
      \u043F\u0440\u0435\u0434\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u0441 \u043D\u0430\u0448\u0435\u0439
      \u0441\u0442\u043E\u0440\u043E\u043D\u044B, \u043D\u0435 \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u0435? "
    exit: 0
    playerAnswer:
